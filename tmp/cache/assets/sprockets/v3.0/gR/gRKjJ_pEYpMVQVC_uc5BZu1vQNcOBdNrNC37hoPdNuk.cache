    Cropper.prototype.update = function(coords) {
      $('#user_crop_x').val(coords.x);
      $('#user_crop_y').val(coords.y);
      $('#user_crop_w').val(coords.w);
      return $('#user_crop_h').val(coords.h);
    };

    return Cropper;

  })();

  new Cropper($('#cropbox'));

  console.log($('.jcrop-box'));

  $('.fakemodal-box').scroll();

}).call(this);
